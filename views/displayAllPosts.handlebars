<div class="blogPosts">
  {{#each serializedPosts}}
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
        <a href="/post/{{id}}">{{title}}</a>
      </h2>
      <div>
        Posted by {{User.username}} 
      </div>
      <p>{{format_date createdAt}}</p>
    </div>
    <div class="card-body">
      {{blogText}}
    </div>
  </article>
  {{/each}}
</div>
